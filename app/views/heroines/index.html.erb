<h2>All Heroines</h2>

<h3>Search by Superpower</h3> 
<%= form_with(url: heroines_path, method: "get") do %>
<%= text_field_tag(:search, params[:search]) %>
<%= submit_tag("Search") %>
<% end %>




<ul>
  <% @heroines.each do |heroine| %>
    <li>
     <%= link_to  heroine.super_name, heroine_path(heroine) %> <%=  %>
    </li>
    <br>
  <% end %>
</ul>
