<h1> Create a Heroine </h1>
	<%= form_for @heroine do |f| %>
	  <%= f.label :name %><br>
	  <%= f.text_field :name %><br>
	 
	  <%= f.label :super_name %><br>
	  <%= f.text_field :super_name %><br>

      <%= f.collection_select(:power_id, Power.all, :id, :name, {:selected => "select one"}) %>
	 
	  <%= f.submit %>
	<% end %>